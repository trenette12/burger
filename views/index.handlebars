<h1>Are you hungry?</h1>
<h3>Choose a Hamburger</h3>
<ul>
  {{#each burger}}
  	<li>
  	   {{this.burger_name}}
    </li>
  {{/each}}
</ul>
<ul>
  {{#each burger}}
    {{#if this.devoured}}
      <li>{{this.burger_name}}</li>
    {{/if}}
  {{/each}}
</ul>
<form action="/" method="POST">
	<textarea type="text" name="event"></textarea>
	<button type="submit">Submit</button>
</form>
